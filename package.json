{
  "name": "browser-cachier",
  "version": "1.1.1",
  "description": "Simple, manually controlled Cache Storage library",
  "main": "./src/cachier.js",
  "scripts": {
    "build": "webpack",
    "lint": "eslint ./src --max-warnings 0",
    "esdoc": "esdoc",
    "test": "jest",
    "test-ci": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tugayilik/cachier"
  },
  "keywords": [
    "cache storage",
    "service worker",
    "offline",
    "cache",
    "pwa",
    "progressive web",
    "progressive web app"
  ],
  "author": "Tugay Ä°lik",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.8.0",
    "jest": "^21.2.1",
    "webpack": "^3.6.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/__test__/src/*.js"
    ],
    "setupFiles": [
      "<rootDir>/__test__/setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "test"
    ],
    "moduleNameMapper": {
      "@@(.*)$": "<rootDir>/src$1"
    }
  }
}
